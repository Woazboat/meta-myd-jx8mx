@@ -32,8 +32,8 @@
 		stby-gpio = <&gpio3 24 GPIO_ACTIVE_HIGH>;
 		reset-gpio = <&gpio4 29 GPIO_ACTIVE_HIGH>;
 		dsi-lanes = <4>;
-		panel-width-mm = <1280>;
-		panel-height-mm = <1024>;
+		panel-width-mm = <396>;
+		panel-height-mm = <324>;
 		
 		toshiba,debug = <1>;
 		toshiba,ppi_tx_rx_ta = <0x00050006>;
